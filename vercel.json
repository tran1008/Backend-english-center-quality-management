{
    "version": 2,
    "builds": [
      {
        "src": "src/api/helpers/connectDB.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/api/helpers/connectDB.js",
        "methods": ["GET", "POST","PUT", "PATCH", "DELETE"]
      }
    ]
  }